{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [],
  "globalEnv": [
    "CONTENTFUL_DELIVERY_TOKEN",
    "CONTENTFUL_ENV",
    "CONTENTFUL_PREVIEW_TOKEN",
    "CONTENTFUL_SPACE_ID",
    "CONTENTFUL_USE_PREVIEW",
    "DEFAULT_SITE_ID",
    "DEPLOY_URL",
    "DEPLOY_URL",
    "GRAPHQL_SERVER_URL",
    "NODE_ENV",
    "SITE",
    "SITE_SETTINGS",
    "VERCEL_URL",
    "VERCEL_URL",
    "EXAMPLE_EXCLUDED_VARIABLE"
  ],
  "pipeline": {
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:e2e": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:e2e:record": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "build-storybook": {
      "dependsOn": ["^build"],
      "outputs": ["storybook-static/**"]
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    }
  }
}
