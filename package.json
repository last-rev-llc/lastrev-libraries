{
  "name": "lastrev-libraries",
  "version": "0.0.1",
  "main": "index.js",
  "author": "LastRev",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build --output-logs=new-only",
    "clean": "turbo run clean && rm -rf node_modules/ && yarn install",
    "deps:check": "manypkg check",
    "deps:fix": "manypkg fix",
    "dev": "turbo run dev --parallel --output-logs=new-only",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,md,json}\"",
    "release": "yarn build && changeset publish",
    "start": "turbo run start",
    "test:watch": "turbo run test:watch",
    "test": "turbo run test:e2e test",
    "posttest": "yarn combine:reports",
    "version-packages": "changeset version && yarn prettier",
    "combine:reports": "node scripts/combineCoverage.js"
  },
  "private": true,
  "resolutions": {
    "@types/react": "^17.0.0",
    "lodash-es": "^4.17.19",
    "lodash": "^4.17.19"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Opera versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 ChromeAndroid versions"
  ],
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.12.13",
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@manypkg/cli": "^0.21.3",
    "@manypkg/get-packages": "^1.1.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^14.14.35",
    "@types/prettier": "^1.16.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.1.0",
    "commitlint": "^19.2.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.2",
    "eslint-plugin-react-hooks": "^4.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^16.1.2",
    "npm-delay": "^1.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "^2.0.5",
    "prettier-eslint": "^16.3.0",
    "resolve": "^1.15.1",
