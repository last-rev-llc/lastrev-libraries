{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": [
    "ANALYZE_BUNDLE",
    "CONTENTFUL_DELIVERY_TOKEN",
    "CONTENTFUL_ENV",
    "CONTENTFUL_PREVIEW_TOKEN",
    "CONTENTFUL_SPACE_ID",
    "CONTENTFUL_USE_PREVIEW",
    "DEFAULT_SITE_ID",
    "DEPLOY_URL",
    "GRAPHQL_SERVER_URL",
    "PAGES_REVALIDATE",
    "PORT",
    "SITE_SETTINGS",
    "SITE",
    "CONTENTFUL_SETTINGS_ID",
    "NEXT_PUBLIC_SENTRY_DSN"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "gql:sync": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "gql:dev": {
      "dependsOn": ["^build", "gql:sync"],
      "cache": false,
      "persistent": true
    },
    "gql:logs": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "web#build": {
      "dependsOn": ["@graphql-sdk#gql:start", "^build"],
      "outputs": ["out/**", ".next/**"]
    }
  }
}
