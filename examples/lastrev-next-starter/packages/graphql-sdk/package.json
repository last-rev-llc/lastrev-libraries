{
  "name": "@lrns/graphql-sdk",
  "version": "0.1.0",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean codegen build:prod",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "clean": "rimraf dist",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "dev": "concurrently \"cross-env NODE_ENV=development rollup -cw\" \"yarn codegen:watch\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "@apollo/client": "^3.3.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "graphql": "^15.4.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "tsc-watch": "^4.2.9"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-graphql-request": "3.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@last-rev/rollup-config": "^0.0.4",
    "dotenv": "^10.0.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.4",
    "npm-run-all": "^4.1.5",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.7",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.3",
    "graphql": "^15.5.1",
    "nodemon": "^2.0.7",
    "react-scripts": "^4.0.1",
    "tsc-watch": "^4.2.9",
    "@lrns/graphql-runner": "0.1.0",
    "@lrns/graphql-extensions": "0.1.0"
  }
}
